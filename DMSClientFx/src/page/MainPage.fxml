<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainPageController">
   <children>
      <AnchorPane maxWidth="-Infinity" minWidth="150.0" prefHeight="200.0" prefWidth="200.0">
         <children>
            <ListView fx:id="NavigationListView" prefHeight="400.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </AnchorPane>
      <AnchorPane minHeight="-Infinity" minWidth="-Infinity" style="-fx-border-color: gray; -fx-border-radius: 3;">
         <children>
            <TabPane fx:id="MainTabPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" tabClosingPolicy="ALL_TABS" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <tabs>
                <Tab text="생활관 신청">
                     <content>
                        <fx:include source="SubmitApplicationTab.fxml" />
                     </content>
                </Tab>
                <Tab text="생활관 신청 조회">
                     <content>
                        <fx:include source="CheckApplicationTab.fxml" />
                     </content>
                </Tab>
              </tabs>
            </TabPane>
         </children>
         <HBox.margin>
            <Insets bottom="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </AnchorPane>
   </children>
</HBox>
